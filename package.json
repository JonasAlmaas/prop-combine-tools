{
  "name": "prop-combine-tools",
  "version": "1.1.4",
  "description": "A simple to use tool that helps you make the files needed for prop combine to work in cs:go",
  "main": "scripts/main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . prop-combine-tools --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Prop Combine Tools\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonasAlmaas/prop-combine-tools.git"
  },
  "author": "Jonas Alm√•s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JonasAlmaas/prop-combine-tools/issues"
  },
  "homepage": "https://github.com/JonasAlmaas/prop-combine-tools#readme",
  "dependencies": {
    "electron": "^13.6.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "electron-packager": "^15.2.0"
  }
}
